    android:layout_x="0dp"
    android:layout_y="291dp"
    tools:layout_editor_absoluteX="8dp"
    tools:layout_editor_absoluteY="242dp"/>

  <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
